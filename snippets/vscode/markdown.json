{
  "TheoremEnv shortcode": {
    "prefix": "theorem",
    "body": [
      "{% theorem(class=\"${1:class}\") %}",
      "${2:theorem content here.}",
      "{% end %}"
    ],
    "description": "Zola theorem shortcode block"
  },
